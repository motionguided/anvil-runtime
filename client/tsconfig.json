{
    "extends": "./tsconfig.base.json",
    "composite": true,
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@Sk": ["./js/@Sk"],
            "PyDefUtils": ["./js/PyDefUtils.js"],
            "@runtime/*": ["./js/*"],
            "@runtime-assets/*": ["./*"]
